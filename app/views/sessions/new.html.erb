<div class="movie-box">
  <%= form_tag session_path, class: "form-style" do %>
    <div>
      <%= label_tag "email" %><br/>
      <%= text_field_tag "email" %>
    </div>
    <div>
      <%= label_tag :password %><br/>
      <%= password_field_tag :password %>
    </div>
    <div><%= submit_tag "Log In" %></div>
<% end %>
</div>